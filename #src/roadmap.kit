<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="./css/styles.css" />
    <title>Roadmap</title>
  </head>
  <body>
    <body id="roadmap" class="light">
      <div class="body__wrapper roadmap">
        <!-- @import "header.html" -->
        <main class="roadmap">
          <div class="main__wrapper roadmap">
            <h1 class="page__title">ROADMAP <span>//</span></h1>
            <div class="main__grid">
              <div class="roadmap__ref" id="value">
                <div class="ref__title__block main__container">
                  <h3 class="ref__title">Value</h3>
                  <span class="ref__number">01</span>
                </div>
              </div>
              <div class="roadmap__ref" id="community">
                <div class="ref__title__block main__container">
                  <h3 class="ref__title">Community</h3>
                  <span class="ref__number">02</span>
                </div>
              </div>
              <div class="roadmap__ref" id="merch">
                <div class="ref__title__block main__container">
                  <span class="ref__number">03</span>
                  <h3 class="ref__title">Merch</h3>
                </div>
              </div>
              <div class="roadmap__ref" id="metaverse">
                <div class="ref__title__block main__container">
                  <span class="ref__number">04</span>
                  <h3 class="ref__title">Metaverse</h3>
                </div>
              </div>
              <div class="roadmap__ref" id="giveaways">
                <div class="ref__title__block main__container">
                  <h3 class="ref__title">Giveaways & Airdrops</h3>
                  <span class="ref__number">05</span>
                </div>
              </div>
              <div class="roadmap__ref" id="add_on_mind">
                <div class="ref__title__block main__container">
                  <h3 class="ref__title">Have something to add on mind?</h3>
                  <span class="ref__number">06</span>
                </div>
                <span class="icon-caret_right_ic"></span>
              </div>
              <div class="roadmap__frame__container">
                <!-- roadmap frmaes renders by roadmap.js and use data from roadmap_data -->
              </div>
            </div>
          </div>
        </main>
      </div>
      <script src="./js/roadmap_data.js"></script>
      <script src="./js/roadmap.js"></script>
      <script src="./js/main.js"></script>
    </body>
  </body>
</html>
